	    <fileExists path="${installdir}/apache/conf/addons" />
            <fileTest path="${installdir}/apache/conf/addons" condition="is_empty" />
	</ruleList>
    </deleteFile>

    <!-- Clear out any residual registry entres -->
    <actionGroup>
      <actionList>
	    <registryDelete key="HKEY_LOCAL_MACHINE\SOFTWARE\EnterpriseDB\ApacheHTTPD" />
      </actionList>
      <ruleList>
        <compareText>
          <logic>equals</logic>
          <text>${platform_name}</text>
          <value>windows</value>
        </compareText>
      </ruleList>
    </actionGroup>

  </preUninstallationActionList>
